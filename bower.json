{
  "name": "steel-components",
  "version": "0.0.1",
  "description": "A collection of UI components built on top of Metal.js (JavaScript) and Bootstrap 3 (HTML/CSS)",
  "moduleType": [
    "es6"
  ],
  "keywords": [
    "steel",
    "metal",
    "metaljs"
  ],
  "ignore": [
    "*",
    "!build/**/*"
  ],
  "dependencies": {
    "steel-autocomplete": "metal/steel-autocomplete#master",
    "steel-cell": "metal/steel-cell#master",
    "steel-list": "metal/steel-list#master",
    "steel-modal": "metal/steel-modal#master",
    "steel-switcher": "metal/steel-switcher#master",
    "steel-tooltip": "metal/steel-tooltip#master",
    "steel-tooltip-menu": "metal/steel-tooltip-menu#master",
    "steel-treeview": "metal/steel-treeview#master"
  }
}
